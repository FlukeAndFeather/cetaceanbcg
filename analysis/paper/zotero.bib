	keywords = {open science, reproducible research, code access, data access, reproducibility policy},
	pages = {2584--2589},
	file = {Full Text PDF:/Users/frank/Zotero/storage/UHT7RLZ6/Stodden et al. - 2018 - An empirical analysis of journal policy effectiven.pdf:application/pdf;Snapshot:/Users/frank/Zotero/storage/JK4ISN3N/2584.html:text/html},

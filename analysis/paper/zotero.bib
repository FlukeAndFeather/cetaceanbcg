	author = {Cade, David E. and Gough, William T. and Czapanskiy, Max F. and Fahlbusch, James A. and Kahane-Rapport, Shirel R. and Linsky, Jacob M. J. and Nichols, Ross C. and Oestreich, William K. and Wisniewska, Danuta M. and Friedlaender, Ari S. and Goldbogen, Jeremy A.},
	month = aug,
	year = {2021},
	keywords = {Bio-logging, CATS videos, Kinematic sensors, MATLAB, Sensor integration, Tag tutorial, Video processing, Video tags},
	pages = {34},
	file = {Full Text PDF:/Users/frank/Zotero/storage/V6W946QB/Cade et al. - 2021 - Tools for integrating inertial sensor data with vi.pdf:application/pdf;Snapshot:/Users/frank/Zotero/storage/DTW7KI5R/s40317-021-00256-w.html:text/html},
}

